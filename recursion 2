package start;
//find nth fibannachi no
class Practice2 {
    public static void main (String [] args) {
        System.out.println(fibo(9));
    
    }
        static int fibo(int n) {
        //base condition where return value is same
         if(n<3){
            return n;
         }
        return fibo(n-1)+fibo(n-2);

        }

}
